<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mis2_layout"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_below="@+id/total_layout"
    android:layout_marginTop="10dp"
    android:background="@color/light_line"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dip"
        android:weightSum="6.1"
        android:background="@color/white"
        android:orientation="horizontal" >

        <ImageView
            android:layout_width="@dimen/stroke_dim"
            android:layout_height="fill_parent"
            android:background="@color/light_line"
            android:contentDescription="@string/img" />

        <TextView
            android:id="@+id/mis1_cle_header_textview"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.9"
            android:background="@color/light_bg"
            android:gravity="left"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="@string/cls_cles"
            android:textColor="@color/black"
            android:textSize="@dimen/mis_val_size"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="@dimen/stroke_dim"
            android:layout_height="fill_parent"
            android:background="@color/light_line"
            android:contentDescription="@string/img" />

        <TextView
            android:id="@+id/mis2_cle_header_textview"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.9"
            android:background="@color/light_bg"
            android:gravity="left"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="@string/cls_verifiees"
            android:textColor="@color/black"
            android:textSize="@dimen/mis_val_size"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="@dimen/stroke_dim"
            android:layout_height="fill_parent"
            android:background="@color/light_line"
            android:contentDescription="@string/img" />

        <TextView
            android:id="@+id/mis3_cle_header_textview"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.9"
            android:background="@color/light_bg"
            android:gravity="left"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="@string/cls_nonverifiees"
            android:textColor="@color/black"
            android:textSize="@dimen/mis_val_size"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="@dimen/stroke_dim"
            android:layout_height="fill_parent"
            android:background="@color/light_line"
            android:contentDescription="@string/img" />

        <TextView
            android:id="@+id/mis4_cle_header_textview"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.6"
            android:background="@color/light_bg"
            android:gravity="left"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="@string/cls_total"
            android:textColor="@color/black"
            android:textSize="@dimen/mis_val_size"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="@dimen/stroke_dim"
            android:layout_height="fill_parent"
            android:background="@color/light_line"
            android:contentDescription="@string/img" />

        <TextView
            android:id="@+id/mis5_cle_header_textview"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"
            android:background="@color/light_bg"
            android:gravity="left"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="@string/cls_hs"
            android:textColor="@color/black"
            android:textSize="@dimen/mis_val_size"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="@dimen/stroke_dim"
            android:layout_height="fill_parent"
            android:background="@color/light_line"
            android:contentDescription="@string/img" />

        <TextView
            android:id="@+id/mis6_cle_header_textview"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.3"
            android:background="@color/light_bg"
            android:gravity="left"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:text="@string/cls_commentaire"
            android:textColor="@color/black"
            android:textSize="@dimen/mis_val_size"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="@dimen/stroke_dim"
            android:layout_height="fill_parent"
            android:background="@color/light_line"
            android:contentDescription="@string/img" />
    </LinearLayout>

</LinearLayout>